  </Target>
  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="copy &quot;$(SolutionDir)x64\$(Configuration)\ooz.dll&quot; $(TargetDir)" />
  </Target>
</Project>
